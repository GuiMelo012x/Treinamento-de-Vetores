import java.util.Scanner;
public class Vetor2 {
    public static void main(String[] args) throws Exception {
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int[] v = new int [a];
    for (int i = 0;i < v.length;i++){
        v[i] = i;
    }
    for (int i=0;i < v.length;i++){
        System.out.print(v[i]+1 + " ");
    }
    sc.close();
}
}
